@import url(chrome://global/skin/);

treechildren::-moz-tree-checkbox {
  list-style-image: none;
}

treechildren::-moz-tree-checkbox(checked) {
  list-style-image: url("chrome://googlesharing/content/images/cbox-check.gif");
}

tree {min-height: 50px;}

#up-button {
  list-style-image: url("chrome://googlesharing/content/images/up.gif");
}
#down-button {
  list-style-image: url("chrome://googlesharing/content/images/down.gif");
}
